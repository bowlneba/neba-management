<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <RunConfiguration>
    <!-- Browser tests require the Blazor app to be running at http://localhost:5200 -->
    <!-- Exclude them from default 'dotnet test' runs -->
    <!-- Run browser tests explicitly with: dotnet test tests/Neba.BrowserTests/ -->
    <TestCaseFilter>FullyQualifiedName!~BrowserTests</TestCaseFilter>
  </RunConfiguration>
</RunSettings>
